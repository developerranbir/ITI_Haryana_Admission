                    </div>
                <div class="col-lg-3 col-md-3 col-sm-2 col-12 cus-topbar-menu text-right" style="position:absolute;right:0;display:flex;justify-content:flex-end;">
                    <button value="Objection" id="btnobjection" style="display:none" onclick="javascript: return GetCandidateVerifyObjections();"><i class="fa fa-fw fa-warning text-warning"></i>Show Objection</button>
                    <a id="btnLogout" href="@Url.Action("LogOut", "Account")"><i class="fa fa-fw fa-power-off text-danger"></i>Logout</a>
                </div>
            </div>
        </div>
        <div class="">
            <div class="pcoded-wrapper">
                @Html.Partial("_Menu")
