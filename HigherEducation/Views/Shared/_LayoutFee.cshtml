    <!-- Main css -->

    <style>

        .steps li.current a, .steps li.current a:active, .steps li.current a::before {
            background-color: dodgerblue;
        }
        button#btnobjection {
    background: no-repeat;
    color: #fff;
    border: 0;
    margin-top: 10px;
    font-size: 16px;
    font-weight: 700;
    cursor:pointer;
}
       .cus-ol ol {
  list-style: none;
  counter-reset: my-awesome-counter;
  padding:0;
}
.cus-ol ol li {
  counter-increment: my-awesome-counter;
  font-size:14px;
}
        
.cus-ol ol li::before {
    content: counter(my-awesome-counter);
    color: #fcd000;
    font-size: 1rem;
    font-weight: bold;
    line-height: var(--size);
    transform: rotate(-10deg);
    background: black;
    border-radius: 10%;
    text-align: center;
    box-shadow: 1px 1px 0 #999;
    padding: 0.4rem;
    /* width: 32px; */
    margin-right: 10px;
}
.list-group-item:focus, .list-group-item:hover {
    /* text-decoration: none; */
    background-color: #eee;
}
    </style>
