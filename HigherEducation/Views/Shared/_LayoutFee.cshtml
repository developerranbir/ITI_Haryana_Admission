                    <div class="pcoded-inner-content">
                        <div class="col-lg-12 col-md-12 cus-progress">
                            <div class="steps clearfix">
                                <ul role="tablist" id="tablist">
                                    @*<li role="tab" aria-disabled="false" class="" aria-selected="true">
                                        <a id="form-total-t-6" href="@Url.Action("Declaration", "Account")" aria-controls="form-total-p-6">

                                            <span class="current-info audible"> </span>
