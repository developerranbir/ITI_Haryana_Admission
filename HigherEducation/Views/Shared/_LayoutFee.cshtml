            </div>
        </div>

        <div class="modal cus-modal" tabindex="-1" id="myModal1" data-backdrop="static" data-keyboard="false" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <!-- Modal Header -->
                    <div class="modal-header">
                        <h6 class="modal-title">OTP has been sent to your registered mobile no.</h6>
                        <div><b>Resend OTP Time left =</b><span id="timer"></span></div>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <!-- Modal body -->
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-6 form-group">
                                <input type="text" id="txtOtp" maxlength="4" onkeyup="javascript: return checknumbereonly(this);" placeholder="Please Enter OTP" class="form-control" />
                                <span id="errormsg" style="color:red"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-2 form-group">
                                <input type="button" id="btnSubmitOtp" onclick="javascript:return CheckOtp();" class="btn btn-success" value="Submit" />
                            </div>
                            <div class="col-md-2 form-group">
                                <input type="button" id="btnResendOtp" onclick="javascript:return SendOTP();" style="display:none" class="btn btn-info" value="Resend" />
                            </div>
                        </div>
                    </div>

                    <!-- Modal footer -->
                    <div class="modal-footer">
                        <button type="button" class="btn btn-danger" style="min-width: 50px; font-weight: bold; font-size: 12px;" data-dismiss="modal">Close</button>
                    </div>

                </div>
            </div>
        </div>

        <div class="modal cus-modal" tabindex="-1" id="myModal2" data-backdrop="static" data-keyboard="false" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
