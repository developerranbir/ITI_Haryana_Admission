         var url4 = '@Url.Action("FeeReceiptPartial", "Reports")';
        function fetchReceipt(regid,txnID) {
            //alert(regid);
            $("#cover").fadeIn();
            var dataToSend = {};
            dataToSend.regId = regid;
            dataToSend.TxnId = txnID;

            $.ajax({
                //url: "/higher/Account/getdata",
