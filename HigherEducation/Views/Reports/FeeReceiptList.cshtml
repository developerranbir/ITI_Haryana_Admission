            })
        }

        var url2 = '@Url.Action("d_getCollegeListAsPerCollegeType", "Reports")';
        var test = '@Session["UserId"]';
        var C_ID = '@Session["collegeId"]';

        function getCollegeList() {
            $("#cover").fadeIn();

            var dataToSend = {};
            dataToSend.collegeType = $("#ddlCollegeType").val();
            $.ajax({

                url: url2,
                type: 'GET',
                contentType: 'application/json;charset=utf-8',
