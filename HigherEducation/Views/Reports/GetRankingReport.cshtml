    <script>
      var errorFn = function (jqXHR, exception)
        {
            var msg = '';
            if (jqXHR.status === 0) {
                msg = 'Not connect.\n Verify Network.';
            } else if (jqXHR.status == 404) {
                msg = 'Requested page not found. [404]';
            } else if (jqXHR.status == 500) {
                msg = 'Internal Server Error [500].';
            } else if (exception === 'parsererror') {
                msg = 'Requested JSON parse failed.';
            } else if (exception === 'timeout') {
                msg = 'Time out error.';
            } else if (exception === 'abort') {
                msg = 'Ajax request aborted.';
            } else {
                msg = 'Uncaught Error.\n' + jqXHR.responseText;
            }
            alert("Error: " + msg);
            $("#cover").fadeOut("slow");
        }

        $(document).ready(function ()
        {
            getCounselling();
            $("#cover").hide();
        });


        var test = '@Session["UserId"]';
        var C_ID = '@Session["collegeId"]';
        var url_1 = '@Url.Action("d_getCounsellingforRank", "Reports")'; //to fetch college type
        function getCounselling()
        {
            $("#cover").fadeIn();
            $.ajax({
                    url: url_1,
                    type: 'GET',
                    contentType: 'application/json;charset=utf-8',
                    dataType: 'json',
                    success: function (data) {
                        var ConVal= JSON.parse(data);
                        $("#ddlCon").empty();
                        if (C_ID == '0') {
                            $("#ddlCon").append('<option value="0">Select All</option>');
                        }
                        else {
                            $("#ddlCon").append('<option value="">Please select Counselling</option>');
                        }
                        $.each(ConVal, function (i) {
                            $("#ddlCon").append(`<option value='` + ConVal[i].counselling + `'>` + ConVal[i].counselling + `</option>`);
                        });

                        if (test != 'dhepkl' && test != '') {
                            $("#ddlCon").val(C_ID);
                            $("#ddlCon").change();
                        }
                        $("#cover").fadeOut();
                    },
                    error: errorFn
                });
            }

        var url5 = '@Url.Action("GetRanking", "Reports")';
        var urlError = '@Url.Action("Error", "Account")';

        function getData() {

            $("#cover").fadeIn();
            var dataToSend = {};
            dataToSend.classType = $("#ddlClass").val();
            dataToSend.counselling = $("#ddlCon").val();
            var CompleteDataSet = {};
            $.ajax({
                url: url5,
                type: 'POST',
                contentType: 'application/json;charset=utf-8',
                dataType: 'json',
                data: JSON.stringify(dataToSend),
                success: function (data) {
                    if (data == "1") {
                        window.location.replace(urlError);
                        return false;
                    }
                    CompleteDataSet = JSON.parse(data);
                    //console.log(CompleteDataSet);
                    var buttonsArray = [{ text: '<i class="fa fa-file-pdf" style="color:RED;"></i>', extend: 'pdfHtml5', footer: true, title: 'Student Ranking Details', messageTop: 'ITI Name: ', messageBottom: '', pageSize: 'A4', orientation: 'landscape', exportOptions: { columns: ':visible' } }, { title: 'Student Ranking Details', messageTop: 'ITI Name: ', text: '<i class="fa fa-file-excel" style="color:GREEN;"></i>', extend: 'excelHtml5', exportOptions: { columns: ':visible' } }, { text: '<i class="fa fa-file-csv" style="color:BLUE;"></i>', extend: 'csvHtml5', exportOptions: { columns: ':visible' } }, { text: '<i class="fa fa-print" style="color:Black;"></i>', extend: 'print', exportOptions: { columns: ':visible' } }];
                    //debugger;
                    var columnsArray = [
                        { data: null, title: '#', render: function (data, type, row, meta) { return meta.row + meta.settings._iDisplayStart + 1; } }
                        , { data: "Reg_Id", title: "Registration ID", visible: true }
                        , { data: "Candidate_FullName", title: "Candidate FullName", visible: true }
                        , { data: "FatherName", title: "Father Name", visible: true }
                        , { data: "MotherName", title: "Mother Name", visible: true }
                        , { data: "BirthDate", title: "Date of Birth", visible: true }
                        , { data: "sex", title: "Gender", visible: true }
                        , { data: "RegIDNo", title: "Registration ID", visible: true }
                        , { data: "HaryanaDomicileOr10thFromHaryana", title: "Haryana Domicile Or 10th From Haryana", visible: true }
                        , { data: "RankNo", title: "Rank No", visible: true }
                        , { data: "Counselling", title: "Counselling", visible: true }
                        , { data: "Qualification", title: "Qualification", visible: true }
                        , { data: "BasePercentage", title: "Base Percentage", visible: true }
                        , { data: "WeightagePercentage", title: "Weightage Percentage", visible: true }
                        , { data: "Widow_Ward", title: "Widow Ward", visible: true }
                        , { data: "Orphan", title: "Orphan", visible: true }
                        , { data: "PanchayatWeightageClaimed", title: "Panchayat Weightage Claimed", visible: true }
                        , { data: "PanchayatWeightageGiven", title: "Panchayat Weightage Given", visible: true }
                        , { data: "Is_Graduation_Done", title: "Is Graduation Done", visible: true }
                        , { data: "Graduation_Percentage", title: "Graduation Percentage", visible: true }
                        , { data: "Is_Twelth_Done", title: "Is_Twelth Done", visible: true }
                        , { data: "Twelth_Percentage", title: "Twelth Percentage", visible: true }
                        , { data: "Widow_Orphan_weightage", title: "Widow Orphan Weightage", visible: true }
                        , { data: "Panch_WeightagePercentage", title: "Panch Weightage Percentage", visible: true }
                        , { data: "Graduation_Weightage", title: "Graduation Weightage", visible: true }
                        , { data: "Twelth_Weightage", title: "Twelth Weightage", visible: true }
                        , { data: "QualifyingExam", title: "Qualifying Exam", visible: true }
                        , { data: "TotalWeightage", title: "Total Weightage", visible: true }
                        , { data: "mathmarks", title: "Math Marks", visible: true }
                        , { data: "sciencemarks", title: "Science Marks", visible: true }
                        , { data: "englishmarks", title: "English Marks", visible: true }

                    ];

                    $("#tblRankingDetails").DataTable({
                        data: CompleteDataSet.Table,
                        dom: 'Blfrtip',
                        buttons: buttonsArray,
                        paging: true,
                        pageLength: 10,
                        lengthMenu: [
                            [10, 25, 50, 100, -1],
                            [10, 25, 50, 100, 'All'],
                        ],
                        destroy: true,
                        pageSize: 'A4',
                        "bFilter": false,
                        "searching": true,
                        scrollX: true,
                        //"order": [[1, "asc"]],
                        columns:columnsArray,
                        "footerCallback": function (row, data, start, end, display) {

                        }

                    });

                    $("#cover").fadeOut("slow");
                },
                error: errorFn
        })
        }
    </script>

