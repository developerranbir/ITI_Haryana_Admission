                { data: "APIResponse", title: "Data Uploaded", },
                { data: "admStatus", title: "Admission Status", },
                { data: "prnGenerated", title: "PRN Generated", },
                //{ data: 'Reg_ID', title: 'Action', render: (d, t, r) => { return '<i  class="btn cus-viewbtn" onclick="viewForm(this);" id="' + d + '" value="' + d + '">Edit / View</i>'; } }
                {
                    data: 'Reg_ID', title: 'Action', render: (d, t, r) => {
                        if (r.admStatus === 'Active' && r.prnGenerated !== 'Yes') {
                            return `<i class="btn cus-viewbtn" onclick="viewForm(this);" id="${d}" value="${d}">Edit / View</i>`;
                        } else {
                            // Hide the button or return empty if status is not 'Active'
                            return '';
                        }
                    }
                }
                  
            ];
            $("#CandidateDetails").DataTable({
                data: CompleteDataSet.Table,
                dom: 'Blfrtip',
                buttons: buttonsArray,
                paging: true,
                pageLength: 10,
                lengthMenu: [
                    [10, 25, 50, 100, -1],
                    [10, 25, 50, 100, 'All'],
                ],
                destroy: true,
                pageSize: 'A4',
                "bFilter": false,
                "searching": true,
                //bPaginate: true,
                //"order": [[1, "asc"]],
                columns: columnsArray,
                columnDefs: [
                    { orderable: false, targets: -1 }
                ],
                "drawCallback": function (settings) {
                },
            });
            $("#cover").fadeOut();
        },
        error: errorFn
    })
    }

    function viewForm(btn) {
        window.location = "../APIData/viewCandidateDetailsITI?RegID=" + btn.id;
    }


</script>