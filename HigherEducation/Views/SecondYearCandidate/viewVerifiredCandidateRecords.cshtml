<script>
    var errorFn = function (jqXHR, exception) {
        var msg = '';
        if (jqXHR.status === 0) {
            msg = 'Not connect.\n Verify Network.';
        } else if (jqXHR.status == 404) {
            msg = 'Requested page not found. [404]';
        } else if (jqXHR.status == 500) {
            msg = 'Internal Server Error [500].';
        } else if (exception === 'parsererror') {
            msg = 'Requested JSON parse failed.';
        } else if (exception === 'timeout') {
            msg = 'Time out error.';
        } else if (exception === 'abort') {
            msg = 'Ajax request aborted.';
        } else {
            msg = 'Uncaught Error.\n' + jqXHR.responseText;
        }
        alert("Error: " + msg);
    }
    var C_ID = '@Session["collegeId"]';
    var test = '@Session["UserId"]';
    $(document).ready(function () {
        //debugger
        $('#Preview').hide();
        $("#cover").hide();
        getAdmissionYear();

        $("#ddlSession").on('change', function () {
            if (C_ID == '0') {
                getCollegeType();
            }
            else {
                getCollegeList1();
                $("#ddlCollege").prop("disabled", true);
            }
        });

        if (C_ID == '0') {
            getCollegeType();
        }
        else {
            getCollegeList1();
            $("#ddlCollege").prop("disabled", true);
        }

        $("#ddlCollegeType").on('change', function () {
            getCollegeList($(this).val());
            $("#ddlCourse").empty();
            $("#ddlCourse").append(`<option value='0'>Select All</option>`);
            $("#ddlSection").empty();
            $("#ddlSection").append(`<option value='0'>Select All</option>`);
        });

        $("#ddlCollege").on('change', function () {
            $("#ddlSection").empty();
            $("#ddlSection").append(`<option value='0'>Select All</option>`);
            getCourseListArPerCollege($(this).val());
        });

        $("#ddlCourse").on('change', function () {
            $("#ddlSection").empty();
            $("#ddlSection").append(`<option value='0'>Select All</option>`);
            getSectionListAsPerCourse($('#ddlCollege').val(), $(this).val(), $("#ddlSession").val());
            //getData();
        });
    });


    var url1 = '@Url.Action("getAdmissionSession", "SecondYearCandidate")'; //to fetch admission Session
    function getAdmissionYear() {
        $("#cover").fadeIn();
        $.ajax({
            url: url1,
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function(data) {
                var admissionYear = JSON.parse(data);
                $("#ddlSession").empty();
                $("#ddlSession").append('<option value="0">Please select</option>');
                $.each(admissionYear, function(i) {
                    $("#ddlSession").append(`<option value='` + admissionYear[i].session_id + `'>` + admissionYear[i].session_year + `</option>`);
                });
                $("#cover").fadeOut();
            },
            error: errorFn
        });
    }

    var url2 = '@Url.Action("d_getCollegeType", "SecondYearCandidate")'; //to fetch college type
    function getCollegeType() {
        $("#cover").fadeIn();
        $.ajax({
            url: url2,
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            success: function(data) {
                var TypeOfCollege = JSON.parse(data);
                $("#ddlCollegeType").empty();
                if (C_ID == '0') {
                    $("#ddlCollegeType").append('<option value="0">Select All</option>');
                } else {
                    $("#ddlCollegeType").append('<option value="">Please select college Type</option>');
                }
                $.each(TypeOfCollege, function(i) {
                    $("#ddlCollegeType").append(`<option value='` + TypeOfCollege[i].collegetypeid + `'>` + TypeOfCollege[i].collegetype + `</option>`);
                });
                $("#ddlCollegeType").val('1').change();
                if (test != 'dhepkl' && test != '') {
                    $("#ddlCollegeType").val(C_ID);
                    $("#ddlCollegeType").change();
                }
                $("#cover").fadeOut();
            },
            error: errorFn
        });
    }

    var url3 = '@Url.Action("GetCollegeList", "SecondYearCandidate")';  // for ITI Level Names List
    function getCollegeList1() {
        var dataToSend = {};
        dataToSend.sessionId = $("#ddlSession").val();
        $.ajax({
            url: url3,
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            data: dataToSend,
            dataType: 'json',
            success: function(data) {
                var ListOfColleges = JSON.parse(data);
                $("#ddlCollege").empty();
                $("#ddlCollege").append(`<option value=''>Please select college</option>`);
                $.each(ListOfColleges, function(i) {
                    $("#ddlCollege").append(`<option value='` + ListOfColleges[i].collegeid + `'>` + ListOfColleges[i].collegename + `</option>`);
                });
                if (C_ID != '0' && test != '') {
                    //getData();
                            $("#ddlCollege").val(C_ID);
                            $("#ddlCollege").change();
                        }
            },
            error: errorFn
        });
    }

    var url4 = '@Url.Action("getCollegeListAsPerCollegeType", "SecondYearCandidate")';  // for Dept Level ITI names list
    function getCollegeList() {
        $("#cover").fadeIn();
        var dataToSend = {};
        dataToSend.collegeType = $("#ddlCollegeType").val();
        dataToSend.sessionId = $("#ddlSession").val();
        $.ajax({

            url: url4,
            type: 'GET',
            contentType: 'application/json;charset=utf-8',
            data: dataToSend,
            dataType: 'json',
            success: function (data) {
                var ListOfColleges = JSON.parse(data);
                $("#ddlCollege").empty();

                if (C_ID == '0') {
                    $("#ddlCollege").append('<option value="0">Select All</option>');
                }
                else {
                    $("#ddlCollege").append('<option value="">Please select college</option>');
                }
                $.each(ListOfColleges, function (i) {
                    $("#ddlCollege").append(`<option value='` + ListOfColleges[i].collegeid + `'>` + ListOfColleges[i].collegename + `</option>`);
                });

                if (C_ID != '0' && test != '') {
                    $("#ddlCollege").val(C_ID);
                    $("#ddlCollege").change();
                }
                $("#cover").fadeOut();
            },
            error: errorFn
        });
    }

    var url5 = '@Url.Action("GetCourseList", "SecondYearCandidate")';       // Course list as per ITI
    function getCourseListArPerCollege(collegeId) {
        $("#cover").fadeIn();
        var dataToSend = {};
        dataToSend.collegeId = $("#ddlCollege").val();
        dataToSend.sessionId = $("#ddlSession").val();
        $.ajax({
            url: url5,
            type: 'get',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            data: dataToSend,
            success: function(data) {
                var ListOfCourses = JSON.parse(data);
                $("#ddlCourse").empty();
                $("#ddlCourse").append(`<option value='0'>Select All</option>`);
                $.each(ListOfCourses, function(i) {
                    $("#ddlCourse").append(`<option value='` + ListOfCourses[i].courseid + `'>` + ListOfCourses[i].name + `</option>`);
                })
                $("#cover").fadeOut();
            },
            error: errorFn

        })
    }

    var url6 = '@Url.Action("GetSectionList", "SecondYearCandidate")';
    function getSectionListAsPerCourse(collegeId, courseId, sessionId) {
        var dataToSend = {};
        dataToSend.CollegeId = collegeId;
        dataToSend.CourseId = courseId;
        dataToSend.SessionId = sessionId;
        $("#cover").fadeIn();
        $.ajax({
            url: url6,
            type: 'POST',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            data: JSON.stringify(dataToSend),
            success: function(data) {
                var ListOfSections = JSON.parse(data);
                $("#ddlSection").empty();
                $("#ddlSection").append(`<option value='0'>Select All</option>`);
                $.each(ListOfSections, function(i) {
                    $("#ddlSection").append(`<option value='` + ListOfSections[i].SectionId + `'>` + ListOfSections[i].sectionName + `</option>`);
                })
                $("#cover").fadeOut();
            },
            error: errorFn
        })
    }


    var CollegeNameSelected;
    var CourseNameSelected;
    var SectionNameSelected;
    var SessionSelected;
var url7 = '@Url.Action("d_getStudentDetailsYES", "SecondYearCandidate")';
    var CompleteDataSet = '';
    var urlError = '@Url.Action("Error", "Account")';
    function getData() {
        if (C_ID != '0') {
            if ($("#ddlCollege").val() == '' || $("#ddlCollege").val() == '0' || $("#ddlCollege").val() == 'null') {
                swal('Alert!', 'Please Select College ', 'error'); return;
            }
        }

        if ($("#ddlSession").val() == '' || $("#ddlSession").val() == '0' || $("#ddlSession").val() == 'null') {
            swal('Alert!', 'Please Select Admisssion Session', 'error'); return;
        }
        /*
        if ($("#ddlCourse").val() == '' || $("#ddlCourse").val() == '0') {
            swal('Alert!', 'Please Select Course ', 'error'); return;
        }*/
    $("#cover").fadeIn();
    var dataToSend = {};
    dataToSend.CollegeId = $("#ddlCollege").val();
    dataToSend.CourseId = $("#ddlCourse").val();
        dataToSend.SectionId = $("#ddlSection").val();
        dataToSend.SessionId = $("#ddlSession").val();
        if ($("#ddlCollege :selected").text() == 'Select All') {
            CollegeNameSelected = 'All ITI';
        }
        else {
            CollegeNameSelected = $("#ddlCollege :selected").text();
        }
        if ($("#ddlCourse :selected").text() == 'Select All') {
            CourseNameSelected = 'All Courses';
        }
        else {
            CourseNameSelected = $("#ddlCourse :selected").text();
        }
        if ($("#ddlSection :selected").text() == 'Select All') {
            SectionNameSelected = 'All Section';
        }
        else {
            SectionNameSelected = $("#ddlSection :selected").text();
        }
        SessionSelected = $("#ddlSession :selected").text();

    $.ajax({
        url: url7,
        type: 'POST',
        contentType: 'application/json;charset=utf-8',
        dataType: 'json',
        data: JSON.stringify(dataToSend),
        success: function(data) {
            if (data == "1") {

                window.location.replace(urlError);
                return false;
