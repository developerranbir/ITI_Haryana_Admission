<script type="text/javascript">
    var aa;

    $(document).ready(function ()
    {
        var MaxPages = '@Session["MaxPage"]';
        var currentPage = '@Session["currentPage"]';

        for (var i = 0; i < parseInt(MaxPages); i++)
        {
            if ((i+1) != currentPage) $("#tablist > li:eq(" + i + ")").addClass('done');
            else $("#tablist > li:eq(" + i + ")").addClass('current');
        }
        for (i = 8; i > MaxPages; i--) { $("#tablist > li:eq(" + (i - 1) + ")>a").attr("href", "#"); }

        var urlForDecData = "@Url.Action("getDeclarationDataRAJ", "Account")";
          $.ajax({
                type: "POST",
                url: urlForDecData,
                data: {},
                contentType: "application/json; charset=utf-8",
                dataType: "json",
              success: function (data)
              {
                  if (data == "[]" || data == "{}")
                  {
                    alert("data Not found for this record! Application Failed");
                    return;
                  }

                  var issueList = '';
                  var issueNumber = 0;

                  var response_DataSet = JSON.parse(data);
                   $('#submitdate').text("Form Submitted on: " +response_DataSet.Table[0].DecSubmitDate) ;
                  $.each(response_DataSet.Table, function (k, v)
                  {
                      $.each(v, function (key, value)
                      {
                          //console.log(key + ": " + value);
                          if (key == 'AadhaarNo' && value !== null && value !== '')
                          {
                              $('#' + key).text('XXXXXXXX' + value.substr(8, 12));
                          }
                          else if (key == 'BirthDate')
                          {
                              var d = new Date(value);
                              //var d = value;
                              var BDate = d.getDate() < 10 ? ('0' + d.getDate()) : d.getDate();
                              Tmonth = d.getMonth() + 1;
                              var BMonth = Tmonth < 10 ? ('0' + Tmonth) : Tmonth;
                              $('#' + key).text(BDate + '/' +  BMonth + '/' + d.getFullYear());
                          }
                          else if (key == "Caste" || key == "CasteCasteCateogry" || key == "CandidateName" || key =="BirthDate" )
                          {
                              if (value == null || value == '')
                              {
                              //    swal('error', "Please check your Caste, Caste Category, Candidate Name, Date of Birth, Application will not be submitted", "error");
                                  issueList += (++issueNumber) + '. Value not found in ' + key;
                                  $('#backreset').remove();
                              }
                              $('#' + key).text(value == null || value == '' ? '---' : value);
                          }
                          else
                          {
                              $('#' + key).text(value==null||value==''?'---':value);
                          }

                      });
                  })

                  $("#Gap_Year").text(response_DataSet.Table2.Gap_Year);// This will Set Gap_year of the student

                 ////ABCD
                  $("#tblSubjectDetails").DataTable({
                      dom: 't',
                      paging: false,
                      sorting:false,
                      data: (response_DataSet.Table1),
                      columns: [
                          {data: 'SubjectName'},
                          {data: 'MarksObt'},
                          {data: 'MaxMarks'},
                      ]
                  });

                  $("#tblfeedetail").DataTable({
                      dom: 't',
                      paging: false,
                      sorting: false,
                      data: (response_DataSet.Table5),
                      columns: [
                          { data: 'Amount' },
                          { data: 'tracking_id' },
                          { data: 'bank_ref_no' },
                          { data: 'order_status' },
                          { data: 'payment_mode' },
                          { data: 'CreateDate' },
                          { data: 'Payment_transactionId' },
                      ]
                  });


                  $("#tblEduQual").DataTable({
                      dom: 't',
                      paging: false,
                      sorting: false,
                      data: (response_DataSet.Table2),
                      columns: [
                          { data: 'ExamPassed' },
                          { data: 'Board' },
                          { data: 'SchoolCollege' },
                          { data: 'RegistrationRollno' },
                          { data: 'ResultStatus' },
                          { data: 'PassingYear' },
                          { data: 'MarksObt' },
                          { data: 'MaxMarks' },
                          { data: 'Subject' ,visible:false},
                          { data: 'Percentage' },
                      ]
                  });

                  $.each(response_DataSet.Table3, function (k, v)
                  {
                      $.each(v, function (key, value)
                      {
                          console.log(key + ": " + value);

                          $('#' + key).text(value == null || value == '' ? '---' : value);

                      });
                  });

                  for (var i = 0, j = 0; i < response_DataSet.Table4.length; i++)
                  {
                      if (response_DataSet.Table4[i].DocId != 8 && response_DataSet.Table4[i].DocId != 9)//Matric
                      {
                          $("#tblDocs > tbody").append('<tr><td>' + (++j).toString() + '</td><td>' + response_DataSet.Table4[i].docname + '</td><td style="display:none">' + (response_DataSet.Table4[i].documentno == null ? '---' : response_DataSet.Table4[i].documentno) +'</td></tr>');
                      }
                      else if (response_DataSet.Table4[i].DocId == 8)//UserPhoto
                      {
                          addImageToControl("UserPhoto", response_DataSet.Table4[i].Docs);
                      }
                      else if (response_DataSet.Table4[i].DocId == 9)//SignId
                      {
                          addImageToControl("SignId", response_DataSet.Table4[i].Docs);
                      }
                  }
                   //if (sc_show_declaration_flag == 2)
                  //if ($("#tblSubjectDetails > tbody > tr").length < 3 && $('#StreamName').text().toLowerCase() != "graduation" && $('#StreamName').text().toLowerCase() != "8th")
                  //{
                  //    swal("Error!", "Application Cannot be Submitted\nKindly Cross Check No of Subjects in 10th: " + ($("#tblSubjectDetails > tbody > tr").length) +" is less than 3\n", "error");
                  //}
                  if (!($('#CasteCategory').text() == 'Scheduled Caste' && (
                         $('#parental_income').text() == 'Less than or equal to Rs 50,000'
                      || $('#parental_income').text() == 'More than Rs 50,000 and less than or equal to Rs 1,00,000'
                      || $('#parental_income').text() == 'More than Rs 1,00,000 and less than or equal to Rs 1,50,000'
                      || $('#parental_income').text() == 'More than Rs 1,50,000 and less than or equal to Rs 2,00,000'
                      || $('#parental_income').text() == 'More than Rs 2,00,000 and less than or equal to Rs 2,50,000'
                  )))
                      $('#dec_sc_post_matric_scholarship').hide();//this is to be shown when caste is SC and Family income is lessthan 2.5 lakhs#Caste,#Parental_Income
                  ////ABCD
                  if (issueNumber > 0)
                  {
                      swal("Error!", "Application Cannot be Submitted\nKindly Cross Check for following:\n" + issueList, "error");
                      return false;
                  }
              }

          });
        $.ajax({
            //url: "/higher/Account/getdata",
            url: '@Url.Action("getdata","Account")',
            type: 'POST',
            contentType: 'application/json;charset=utf-8',
            dataType: 'json',
            data: {},
            success: function (data)
            {
                $("#tblChoice").DataTable({
                    data: JSON.parse(data),
                    dom: 't',
                    paging: false,
                    sorting: false,
                    "order": [[0, "asc"]],
                    columns: [
                        {
                            data: 'pref_no', title: 'Preference No.', render: function (d,t,r)
                            {
                                if (d == '' || d == null)
                                {
                                    //swal('error', "Please check your Preferences, Application will not be submitted", "error");
                                    //$('#backreset').remove();
                                }
                                return d;
                            }},

                        { data: 'collegename', title: 'ITI', render: function (d, t, r) { return d + ', ' + r["districtname"]; } },
                        { data: 'districtname',visible:false },
                        { data: 'statename',visible:false },

                        { data: 'name', visible: false, title: 'Trade', render: function (d, t, r) { return d + ', ' + r['sectionname']; } },
                        { data: 'name', title: 'Trade' },

                        { data: 'sectionname', title:'Trade Section' },
                    ]
                });
            }
        })

        if ($('#cmd').is(':visible')) {
            $('#decCheck').prop('checked',true)
        }

        // disable enter as submit button
        $(window).keydown(function (event) {
            if (event.keyCode == 13) {
                event.preventDefault();
                return false;
            }
        });
    });


    function addImageToControl(imgId,base64)
    {
        var switch_on = base64.toString().substr(0, 5);
        switch (switch_on)
        {
            case 'iVBOR':

                $("#" + imgId).attr("src", "data:image/png;base64," + base64);

                break;
            case '/9j/4':

                $("#" + imgId).attr("src", "data:image/jpeg;base64," + base64);

                break;
            case 'R0lGO':

                $("#" + imgId).attr("src", "data:image/gif;base64," + base64);

                break;

            default:

                $("#" + imgId).attr("src", "data:image/png;base64," + base64);

        }
    }



    $('#cmd').click(function ()
    {
        if ($("#decCheck").prop('checked'))
            window.print();
        else
            swal("Warning", " Kindly Accept the Declaration to continue", "warning");//swal("Caution", "Declaration is not accepted!", "warn");

    });
    @*function printDiv(item)
    {
        var page = '<html><head><title>@Session["RegId"]</title></head><body>';
        page += item.toString();
        page += '</body>';
    }*@

