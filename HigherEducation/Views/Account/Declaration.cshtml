    function confirm_and_submit() {

        var EmailSMS = { 'Mobile': $('#MobileNo').text(), 'Email': $('#Email').text() };

        if ($('#decCheck').is(':not(:checked )')) {

            swal('Alert!', 'Kindly Accept the Declaration to continue', 'warning');
            return;
        }

                    swal({
                        title: "Are you sure?",
                        text: "After final submission you will be not allowed to change the details!",
                        type: "warning",
                        showCancelButton: true,
                        confirmButtonColor: '#DD6B55',
                        confirmButtonText: 'Yes, I am sure!',
                        cancelButtonText: "No, cancel it!",
                        closeOnConfirm: false,
                        closeOnCancel: true,
                        showLoaderOnConfirm: true,
                    },
                        function (isConfirm) {
                            if (isConfirm) {
                              //startPreloader();

                                $.ajax({
                                    type: "POST",
                                    url: "@Url.Action("SaveUserPageDeclaration", "Account")",
                                    data: JSON.stringify(EmailSMS),
                                    contentType: "application/json; charset=utf-8",
                                    dataType: "json",
                                    success: function (data) {

                                        var aa = JSON.parse(data);
                                        if (aa[0].success == "1") {
                                            if (isConfirm) {
                                                FinalMessage();
                                            }

                                        }
                                        else {
                                            swal("Error", "Something went wrong plz try again later.!!", "error");
                                        }

                                    }
                                });
                            }

                        });
    }
    function FinalMessage() {
                    swal({
            title: "Congratulations!!",
            text: "Your form has been successfully submitted. Now you can print your form. ",
            type: "success",
            showCancelButton: false,
            confirmButtonClass: "btn-danger",
            confirmButtonText: "OK",
            closeOnConfirm: false
                    },
              function (isConfirm) {
                if (isConfirm) {

                    window.location = "@Url.Action("Declaration", "Account")"
                }
                else {

                }
            });

        }
</script>
