    <div class="row" style="margin-bottom:4%;">
        @if (Session["Verificationstatus"].ToString() == "")
        {
            <span class="text-monospace text-dark container-fluid"><b> Form Generated on :</b> @DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt")</span>
        }
        else
        {
            <span class="text-monospace text-dark container-fluid" id="submitdate"><b></b></span>
            @*<span class="text-monospace text-dark container-fluid"><b> Form Generated on :</b> @DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt")</span>*@
        }

        @if (Session["Verificationstatus"].ToString() == "" || Session["Verificationstatus"].ToString() == "R")
        {
            if (Session["RemainingRegFee"].ToString() == "Y")
            {
                <div class="col-md-12 text-center cus-registerbtn">
                    <span style="color:red; font-weight:bold">You have to pay the remaining registration fee. <a href="@Url.Action("RegistrationFee","RegistrationPayment")" id="remainingfee"><strong>Click here to pay.</strong></a> </span>
