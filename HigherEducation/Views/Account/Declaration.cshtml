        @*
            $.ajax({
                type: "POST",
                //url: "/Account/SaveUserPageDeclaration",
                url: "@Url.Action("SaveUserPageDeclaration", "Account")",
                data: {},
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {

