        function startPreloader() {
            $(".loader").css("display", "block");
            $(".outer-loader").css("display", "block");
        }
        function stopPreloader() {
            $(".loader").css("display", "none");
            $(".outer-loader").css("display", "none");
        }

        function getDocument(docid, controlId)
        {
            $("#"+controlId).attr("src","../assets/images/Download.gif")
            var DataToPost = {};
            DataToPost.DocId = docid;
            DataToPost.regid = $("#RegistrationId").text();
            $.ajax({
                type: "POST",
                url: '@Url.Action("v_GetDocument", "Verification")',
                data: JSON.stringify(DataToPost),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data)
                {

                    if (data == "[]" || data == "{}")
                    {
                        swal("Information", "No Data Found!", "info");
                        $("#" + controlId).attr("src", "../assets/images/no-img.png")
                    }
                    else if (data == "[{\"Docs\":null}]")
                    {
                        swal("Information", "No Document Found! ", "info");
                        $("#" + controlId).attr("src", "../assets/images/no-img.png")
                    }
                    else if (data == "1")
                    {

                        swal("error", "Session has Expired, Kindly Login again", "error");
                        window.location = '/';
                        return;
                    }
                    else
                    {
                        addImageToControl(controlId, JSON.parse(data)[0].Docs);
                        //$("#" + controlId).parent().children('.btn.btn-sm.btn-success').remove();
                        $('#documentModal').modal('show');
                    }

                },
                error:errorFn
            });
