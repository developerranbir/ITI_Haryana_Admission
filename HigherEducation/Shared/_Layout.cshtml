        function OpenCounsellingConsent() {

            $('#btnconsentYes').attr('disabled', 'disabled');
            $('.error').fadeIn();
            $('.error1').fadeIn();
        var radioValue = $("input[name='counselling']:checked").val();
        var radioValueEdit = $("input[name='counsellingEdit']:checked").val();

       if ($("input[name='counselling']").is(':checked') == false) {

           //swal("Alert!", "Do you wants to participate in Open Counselling?", "warning");
           $('.error').text("required");
           $('.error').delay(2000).fadeOut();
           return;
       }
       if ($("input[name='counsellingEdit']").is(':checked') == false && $("input[name='counsellingEdit']").is(':visible')) {

           //swal("Alert!", "Please confirm, Do you wants to edit your form?", "warning");
           $('.error1').text("required");
           $('error1').delay(2000).fadeOut();
           return;
            }
            startPreloader1();

            var objtosend = {};
            objtosend.Counselling = radioValue;
            objtosend.CounsellingEdit = radioValueEdit;

           $.ajax({
               type: "POST",
               url: '@Url.Action("OpenCounsellingConsent", "Account")',
               data: JSON.stringify(objtosend),
               contentType: "application/json; charset=utf-8",
               dataType: "json",
               success: function (data) {
                   if (data == "1") {

                       if (radioValue == "Y") {
                           $("#OpenCounselling").hide();
                           stopPreloader1();
                           FinalMessageOpen();
                       }
                       else {
                           $("#OpenCounselling").hide();
                           stopPreloader1();
                            FinalMessageOpen();
                       }
                   }
                   else if (data=="00") {
                       window.location = "@Url.Action("LogOut", "Account")"
                           stopPreloader1();
                   }
                   else {
                       swal("Alert!",data,"warning");
                   }
               },
               failure: function (data) {
                   stopPreloader1();
                   swal('Error', 'Something went wrong', 'error');
               }
           });

        }

