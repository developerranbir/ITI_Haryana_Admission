        function checknumbereonly(elem) {
            var userinput = $(elem).val();
            var pattern = /^[0-9]*$/i

            if (!pattern.test(userinput)) {
                swal('warning!', 'Only Numbers Allowed.', 'warning');
                $(elem).val('');
                return false;
            }
            else {
                return true;
            }
